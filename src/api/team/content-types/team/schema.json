{
  "kind": "collectionType",
  "collectionName": "teams",
  "info": {
    "singularName": "team",
    "pluralName": "teams",
    "displayName": "Team",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "logotype": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "recruiting": {
      "type": "boolean"
    },
    "invitedPlayers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "inversedBy": "invitedTeams"
    },
    "suggestedPlayer": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::player.player",
      "inversedBy": "enroledTeams"
    },
    "voivodeship": {
      "type": "enumeration",
      "enum": [
        "Dolnośląskie",
        "Kujawsko-pomorskie",
        "Lubelskie",
        "Lubuskie",
        "Łódzkie",
        "Małopolskie",
        "Mazowieckie",
        "Opolskie",
        "Podkarpackie",
        "Podlaskie",
        "Pomorskie",
        "Śląskie",
        "Świętokrzyskie",
        "Warmińsko-mazurskie",
        "Wielkopolskie",
        "Zachodniopomorskie"
      ]
    },
    "city": {
      "type": "string"
    },
    "players": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::player.player",
      "mappedBy": "team"
    },
    "matches": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::match.match",
      "mappedBy": "awayTeam"
    },
    "relatedPosts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "relatedTeams"
    },
    "participatingIn": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::competition.competition",
      "mappedBy": "participatingTeams"
    },
    "invitedTo": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::competition.competition",
      "mappedBy": "invitedTeams"
    },
    "enrolledTo": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::competition.competition",
      "mappedBy": "enrolledTeams"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    }
  }
}
